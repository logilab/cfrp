|count = <%= @count %>|

<%# params: register %>
<div class='register'>
  <% if register.register_image != nil %>
  <div><a href="<%= register.register_image.filepath %>" target="_blank"><img src='<%= register.register_image.filepath %>' width='100' /></a></div>
  <% end %>

  <div><%= register.representation %> <%= register.date %></div>
  <div><%= register.page_text %></div>
  <div class='editor'><%= register.for_editor_notes %></div>
  <div><%= register.misc_notes %></div>
  <div style="clear:both"></div>
</div>

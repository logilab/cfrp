<h1>Listing seating_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @seating_categories.each do |seating_category| %>
  <tr>
    <td><%= seating_category.name %></td>
    <td><%= seating_category.description %></td>
    <td><%= link_to 'Show', seating_category %></td>
    <td><%= link_to 'Edit', edit_seating_category_path(seating_category) %></td>
    <td><%= link_to 'Destroy', seating_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

